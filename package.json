{
  "name": "eslint-plugin-deopt",
  "version": "0.0.1",
  "description": "Plugin for ESLint for find possible deoptimized code",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha",
    "watch": "mocha --watch",
    "build": "babel -d ./lib ./src",
    "prepublish": "npm run lint && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gvozd/eslint-plugin-deopt.git"
  },
  "keywords": [
    "eslint",
    "optimize",
    "optimization",
    "deoptimize",
    "deoptimization"
  ],
  "author": "Gvozd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gvozd/eslint-plugin-deopt/issues"
  },
  "homepage": "https://github.com/Gvozd/eslint-plugin-deopt#readme",
  "engines": {
    "node": ">=4.6.0"
  },
  "peerDependencies": {
    "eslint": ">=3.8.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-plugin-transform-es2015-destructuring": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-register": "6.18.0",
    "eslint": "3.9.1",
    "eslint-config-loris": "6.0.0",
    "mocha": "3.1.2"
